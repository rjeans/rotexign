{
  "name": "Pulse Simulator",
  "author": "jeansy.org",
  "pins": [
    "GND",
    "VCC",
    "",
    "",
    "",
    "PULSE"
  ],
  "controls": [
    {
      "id": "rpm",
      "label": "RPM",
      "type": "range",
      "min": 0,
      "max": 7999,
      "step": 10
    },
    {
      "id": "sweep_enable",
      "label": "RPM Sweep",
      "type": "checkbox"
    },
    {
      "id": "sweep_duration",
      "label": "Sweep Duration (s)",
      "type": "range",
      "min": 5,
      "max": 120,
      "step": 5
    },
    {
      "id": "sweep_min_rpm",
      "label": "Min RPM",
      "type": "range",
      "min": 100,
      "max": 2000,
      "step": 50
    },
    {
      "id": "sweep_max_rpm",
      "label": "Max RPM", 
      "type": "range",
      "min": 1000,
      "max": 8000,
      "step": 100
    },
    {
      "id": "startup_delay",
      "label": "Startup Delay (s)",
      "type": "range",
      "min": 1,
      "max": 10,
      "step": 0.5
    }
  ]
}